                    <Color x:Key="MutedTextColor">#7A90A8</Color>
                    <Color x:Key="DividerColor">#B8D8F0</Color>
                    <Color x:Key="ButtonBackgroundColor">#C8E0F4</Color>
                    <Color x:Key="AccentCyanColor">#0085FF</Color>
                    <Color x:Key="AccentGoldColor">#E8A800</Color>
                    <Color x:Key="AccentGreenColor">#22A860</Color>
                    <Color x:Key="NextPieceBackgroundColor">#C0DCF2</Color>
                    <Color x:Key="OverlayBackgroundColor">#EEF0F4F8</Color>
