<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:SkyDrop"
             x:Class="SkyDrop.App"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="avares://SkyDrop/Styles/FontStyles.axaml"/>
    </Application.Styles>

     <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.ThemeDictionaries>
                <ResourceDictionary x:Key="Dark">
                    <Color x:Key="GameBackgroundColor">#0A0F1A</Color>
                    <Color x:Key="PanelBackgroundColor">#121A2D</Color>
                    <Color x:Key="BoardBackgroundColor">#151E32</Color>
                    <Color x:Key="GridLineColor">#2A3549</Color>
                    <Color x:Key="BorderColor">#3A4A66</Color>
                    <Color x:Key="PrimaryTextColor">#E8EEFF</Color>
                    <Color x:Key="SecondaryTextColor">#8899BB</Color>
                    <Color x:Key="TertiaryTextColor">#667799</Color>
                    <Color x:Key="MutedTextColor">#556688</Color>
                    <Color x:Key="DividerColor">#2A3549</Color>
                    <Color x:Key="ButtonBackgroundColor">#1E2A44</Color>
                    <Color x:Key="AccentCyanColor">#00AAFF</Color>
                    <Color x:Key="AccentGoldColor">#FFD700</Color>
                    <Color x:Key="AccentGreenColor">#4ADE80</Color>
                    <Color x:Key="NextPieceBackgroundColor">#1A2540</Color>
                    <Color x:Key="OverlayBackgroundColor">#EE000000</Color>
                </ResourceDictionary>

